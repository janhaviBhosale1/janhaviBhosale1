#include <bits/stdc++.h>
using namespace std;
int main() {
   int arr[100];
   int n,i;
   cout<<"Enter size of array: ";
   cin>>n;
   cout<<endl;
 //storing elements entered by user in array
   for(i=0;i<n;i++){
       cout<<"Enter elements of array: ";
       cin>>arr[i];
   }
   cout<<"Reverse array is:";
   for(i=n-1;i>=0;i--){
      cout<<endl;
      cout<<arr[i];
      
   }
   return 0;
}
